.container {
  height: 100%;
  background-color: #F6F5F4;
}
.modules {
  display: flex;
  flex-direction: column;
}
.modules view {
  width: 100%;
  height: 80rpx;
  border-bottom: 1rpx solid #C8C8C5;
  display: flex;
  align-items: center;
}
.modules text {
  margin-left: 30rpx;
  font-size: 32rpx;
  color: #333;
}
.title {
  margin-top: 30rpx;
}
.module {
  background-color: #fffffb;
}
.module image {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 30rpx;
}