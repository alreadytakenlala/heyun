.container {
  width: 100%;
  height: 100%;
  background: #F6F5F4;
  padding-right: 30rpx;
}
.items {
  width: 32%;
  height: 100rpx;
  float: left;
}
.items view {
  margin: 30rpx;
  padding: 20rpx;
  margin-right: 0;
  padding-right: 0;
  border: 1rpx solid #E0E0E0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.selected {
  background: #88C058;
}
.items text {
  font-size: 28rpx;
}

/*-- 完成按钮 --*/
.complete {
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
}