camera {
  width: 100%;
  height: 75%;
}
.down_region {
  width: 100%;
  height: 25%;
  display: flex;
  background-color: #000;
  flex-direction: column;
}
.down_region_up {
  width: 100%;
  height: 20%;
  border-bottom: 1rpx solid #333;
  color: #fff;
  font-size: 26rpx;
}
.down_region_down {
  width: 100%;
  height: 80%;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.down_region_down, .down_region_down view, .down_region_down text, .down_region_up {
  display: flex;
  align-items: center;
  justify-content: center;
}
.down_region_down view {
  width: 33%;
  height: 100%;
}
.album text, .complete text {
  color: #fff;
  font-size: 28rpx;
}
.album image, .album text, .complete text {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1rpx solid #939393;
}
.take > view {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border: 1rpx solid #FCFCFC;
}
.take text {
  width: 90%;
  height: 90%;
  color: #000;
  font-size: 28rpx;
  background-color: #fff;
  border-radius: 50%;
}